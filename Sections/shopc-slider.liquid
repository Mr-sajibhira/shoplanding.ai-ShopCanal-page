<link rel="stylesheet" href="{{ 'shopc-style.css' | asset_url }}">
{{ 'swiper.css' | asset_url | stylesheet_tag }}
{{ 'swiper.js' | asset_url | script_tag }}

{% liquid
  assign laptop_padding_top = section.settings.laptop_top
  assign laptop_padding_bottom = section.settings.laptop_bottom
  assign mobail_padding_top = section.settings.mobail_top
  assign mobail_padding_bottom = section.settings.mobail_bottom
  assign bgcolor = section.settings.bgcolor
  assign border_color = section.settings.border_color
  assign border_style = section.settings.border_style
  assign border_width = section.settings.border_width
%}

<style>
  .id_{{ section.id}}{
    background-color: {{bgcolor}};
    box-sizing: border-box;
  }

  .id_{{ section.id}} .container{
      border-color:{{border_color}};
      border-width:{{border_width}};
      border-style:{{border_style}};
      padding-top:{{laptop_padding_top}}px;
      padding-bottom:{{laptop_padding_bottom}}px;
      box-sizing: border-box;
      display: flex;
      flex-direction: column;
  }
           .id_{{ section.id}} .slider_box{
                      display: none;
                   }




  @media(max-width:767px){
      .id_{{ section.id}}{
            padding-left:20px;
       padding-right:20px;
      }
    .id_{{ section.id}} .container{
      padding-top:{{mobail_padding_top}}px;
      padding-bottom:{{mobail_padding_bottom}}px;
    }


  }
</style>

<section class="id_{{ section.id }}">
  <div class="container">
        {% for block in section.blocks %}
          {% case block.type %}
                {% when 'title' %}
              <style>
                .{{block.id}}.title{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.line_height}}px;
                  color:{{block.settings.textcolor}};
                  margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}};
                  order:{{block.settings.title_position}};
                }
                 @media(max-width:767px){
                 .{{block.id}}.title{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_line_height}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}};
                }
                 }
              </style>

              <h3 class="title {{ block.id }}">{{ block.settings.title }}</h3>
       


              {% when 'style' %}
            <style>
                   .id_{{ section.id}} .slider_box{
                    position: relative;
                   height: 100%;
                      display: block;
                   }
                    .id_{{ section.id}}   .swiper {
                       width: 100%;
                        height: 100%;
                       }

              .id_{{ section.id}}   .swiper-slide {
                  display: flex;
                 justify-content: center;
                 align-items: center;
                }
              .id_{{ section.id}}  .swiper-wrapper {
                  align-items: center;
                   }
              
              .id_{{ section.id}} .slider_box .single_slider .row{
                   box-sizing: border-box;
                margin:0px -{{block.settings.gap}}px;
                  
                }
              .id_{{ section.id}} .slider_box  .single_slider .row .col-1{
                padding: 0px {{block.settings.gap}}px;
              }
              .id_{{ section.id}} .slider_box  .single_slider .row .col-2{
                padding: 0px {{block.settings.gap}}px;
              }

               .id_{{ section.id}} .slider_box  .single_slider .row .col{
            
                  box-sizing: border-box;
               }
              .id_{{ section.id}} .slider_box .text{
                   font-size:{{block.settings.text_font_size}}px;
                   font-weight:{{block.settings.text_font_weight}};
                   line-height:{{block.settings.text_line_height}}px;
                   color:{{block.settings.text_color}};
                    margin-bottom:{{block.settings.text_margin_bottom}}px !important;
                     text-align: {{block.settings.align}};
                 }
                      .id_{{ section.id}} .slider_box .text ul{
                   padding-inline-start: 0;
                   margin-block: 0;
                }
                .id_{{ section.id}} .slider_box .text ul li{
                          list-style: none;
                         margin-bottom:10px;
                          display: flex;
                      }
                .id_{{ section.id}} .slider_box .text ul li::before{
                   content: "\2713";
                   font-size:{{block.settings.text_font_size}}px;
                   font-weight:{{block.settings.text_font_weight}};
                   margin-right: 10px;
                   }

              .id_{{ section.id}} .slider_box .img_tag{  
                 aspect-ratio: {{block.settings.img_aspect_ratio}};
                 object-fit: {{block.settings.img_object_fit}};
                 border-radius:{{block.settings.img_border_radius}}{{block.settings.img_border_px}};
              }
              
              .id_{{ section.id}} .slider_box .content h3{
                    font-size:{{block.settings.name_font_size}}px;
                   font-weight:{{block.settings.name_font_weight}};
                   line-height:{{block.settings.name_line_height}}px;
                   color:{{block.settings.name_text_color}};
                   margin-bottom:{{block.settings.name_margin_bottom}}px !important;
              }
             .id_{{ section.id}} .slider_box .content p{
                    font-size:{{block.settings.des_font_size}}px;
                   font-weight:{{block.settings.des_font_weight}};
                   line-height:{{block.settings.des_line_height}}px;
                   color:{{block.settings.des_text_color}};
                   margin-bottom:{{block.settings.des_margin_bottom}}px !important;
              }

              .id_{{ section.id}} .slider_box .profile_img img{
                 aspect-ratio: {{block.settings.profile_img_aspect_ratio}};
                 object-fit: {{block.settings.profile_img_object_fit}};
                 border-radius:{{block.settings.profile_img_border_radius}}{{block.settings.profile_img_border_px}};
                 margin-left:{{block.settings.profile_img_margin_bottom}}px !important;
                width:{{block.settings.profile_img_width}};
              }
              
              .id_{{ section.id}} .slider_box .slider_content{
                 box-sizing: border-box;
                 background-color:{{block.settings.bgcolor}};
                 padding: {{block.settings.padding}}px;
                  
                  border-radius:{{block.settings.border_radius}}px;
                  display: flex;
                   flex-direction: column;
                   align-items: {{block.settings.align}};
              }
              .id_{{ section.id}} .slider_box .slider_content .content_div{ 
               margin:{{block.settings.margin}};
                          }
               .id_{{ section.id}} .slider_box .slider_content .content_div .flex{ 
                   align-items: center;
                 justify-content: {{block.settings.align}};
               }

                 .id_{{ section.id}} .swiper-button-next,  .id_{{ section.id}} .swiper-button-prev {
                      top: auto;
                     bottom: 0;
                   z-index:2;
                   color:{{block.settings.arrow_color}};
                      }
              .id_{{ section.id}} .swiper-pagination-bullet-active {
                 background: {{block.settings.arrow_color}};
                    }
             .id_{{ section.id}}  .swiper-button-prev{
                 left: auto;
                  right: 68px;
                  }
               .id_{{ section.id}}  .swiper-pagination {
                  z-index: 1;
                     text-align: start;
                 bottom: 0;
              }
                .id_{{ section.id}} .swiper{
                  padding:0px 0px 50px 0px;
                }
                    .id_{{ section.id}} .slider_box{
                 margin-bottom:{{block.settings.margin_bottom}}px;
               }
            </style>
            


            
                  {% when 'mobail_style' %}
            <style>
              @media(max-width:767px){ 
                
               .id_{{ section.id}} .slider_box{
                 margin-bottom:{{block.settings.margin_bottom}}px;
               }
                .id_{{ section.id}} .slider_box .single_slider .row{
                  gap:{{block.settings.gap}}px;
                  margin:0px;
                  flex-direction: column-reverse;
                }
              .id_{{ section.id}} .slider_box  .single_slider .row .col-1{
                padding: 0px 0px 0px 0px;
              }
              .id_{{ section.id}} .slider_box  .single_slider .row .col-2{
                padding: 0px 0px 0px 0px;
              }
              
               .id_{{ section.id}} .slider_box .single_slider .row .col{
               
                 max-width:100%;
               }
                
              .id_{{ section.id}} .slider_box .text{
                   font-size:{{block.settings.text_font_size}}px;
                   font-weight:{{block.settings.text_font_weight}};
                   line-height:{{block.settings.text_line_height}}px;
                   margin-bottom:{{block.settings.text_margin_bottom}}px !important;
                   text-align: {{block.settings.align}};
                 }
     
                .id_{{ section.id}} .slider_box .text ul li::before{
                   font-size:{{block.settings.text_font_size}}px;
                   font-weight:{{block.settings.text_font_weight}};
                   }

              .id_{{ section.id}} .slider_box .img_tag{  
                 aspect-ratio: {{block.settings.img_aspect_ratio}};
                 object-fit: {{block.settings.img_object_fit}};
                 border-radius:{{block.settings.img_border_radius}}{{block.settings.img_border_px}};
              }
              
              .id_{{ section.id}} .slider_box .content h3{
                    font-size:{{block.settings.name_font_size}}px;
                   font-weight:{{block.settings.name_font_weight}};
                   line-height:{{block.settings.name_line_height}}px;
                   color:{{block.settings.name_text_color}};
                   margin-bottom:{{block.settings.name_margin_bottom}}px !important;
              }
             .id_{{ section.id}} .slider_box .content p{
                    font-size:{{block.settings.des_font_size}}px;
                   font-weight:{{block.settings.des_font_weight}};
                   line-height:{{block.settings.des_line_height}}px;
                   color:{{block.settings.des_text_color}};
                   margin-bottom:{{block.settings.des_margin_bottom}}px !important;
               }

              .id_{{ section.id}} .slider_box .profile_img img{
                 aspect-ratio: {{block.settings.profile_img_aspect_ratio}};
                 object-fit: {{block.settings.profile_img_object_fit}};
                 border-radius:{{block.settings.profile_img_border_radius}}{{block.settings.profile_img_border_px}};
                 margin-left:{{block.settings.profile_img_margin_bottom}}px !important;
                width:{{block.settings.profile_img_width}};
              }
              
              .id_{{ section.id}} .slider_box .slider_content{
                 box-sizing: border-box;
                 background-color:{{block.settings.bgcolor}};
                 padding: {{block.settings.padding}}px;
                 border-radius:{{block.settings.border_radius}}px;
                 align-items: {{block.settings.align}};
              }
                
              .id_{{ section.id}} .slider_box .slider_content .content_div{ 
               margin:{{block.settings.margin}};
                          }
                
               .id_{{ section.id}} .slider_box .slider_content .content_div .flex{ 
                    justify-content: {{block.settings.align}};
                  }
     .id_{{ section.id}} .swiper-button-next:after,   .id_{{ section.id}} .swiper-button-prev:after {
              font-size: 25px;
             }
                            .id_{{ section.id}} .swiper-button-next,  .id_{{ section.id}} .swiper-button-prev {
           
                     top: 25px;
                            bottom: auto;   
                 
                      }
                
                .id_{{ section.id}}  .swiper-pagination {
                  top:0px;
                 bottom: auto;
              }
                          .id_{{ section.id}} .swiper{
                  padding:50px 0px 0px 0px;
                }

                

              
              }
            </style>
                       {% when 'button' %}
              <style>
           .{{block.id}}{order:{{block.settings.btn_position}};}
                .btn{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.font_size}}px;
                  color:{{block.settings.text_color}};
                  border-color:{{block.settings.border_color}};
                  border-width:{{block.settings.border_width}}px;
                  border-style:{{block.settings.border_style}};
                  border-radius:{{block.settings.border_radius}}px;
                  background-color: {{block.settings.btn_color}};
                  margin-bottom:{{block.settings.margin_bottom}}px;
                  padding:{{block.settings.padding}};
                }
                 .{{block.id}} .btn:hover{
                    background-color: {{block.settings.btn_color_hover}};
                    color:{{block.settings.text_color_hover}};
                    border-color:{{block.settings.border_color_hover}};
                 }
                 .{{block.id}}{
                    text-align: {{block.settings.text_align}};
                 }
                 @media(max-width:767px){
                 .{{block.id}} .btn{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_font_size}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px;
                  padding:{{block.settings.mobail_padding}};
                }
                .{{block.id}}{
                    text-align: {{block.settings.mobail_text_align}};
                 }
                 }
              </style>

              {% if block.settings.btnlink != blank %}
                <div class="{{ block.id }}">
                <a class="btn" href="{{ block.settings.btnlink }}">{{ block.settings.btntext }}</a>
                </div>
              {% endif %}

           {% when 'richtext' %}
              <style>
                 .{{block.id}}{
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   line-height:{{block.settings.line_height}}px;
                   color:{{block.settings.textcolor}};
                   margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}};
                   order:{{block.settings.text_position}};
                 }
                    .{{block.id}} ul{
                             padding-inline-start: 0;
                       margin-block: 0;
                       }
                       .{{block.id}} ul li{
                          list-style: none;
                         margin-bottom:10px;
                          display: flex;
                       }
                 .{{block.id}} ul li::before{

                   content: "\2713";
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   margin-right: 10px;


                   }
                  @media(max-width:767px){
                  .{{block.id}}{
                   font-size:{{block.settings.mobail_font_size}}px;
                   line-height:{{block.settings.mobail_line_height}}px;
                   margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}};
                 }
                .{{block.id}} ul li::before{
                      font-size:{{block.settings.mobail_font_size}}px;
                     }
                  }
              </style>
              <div class="{{ block.id }}">{{ block.settings.text }}</div>

            
          {% endcase %}
      {% endfor %} 


     
       <div class="slider_box">
             

          <div class="swiper mySwiper{{ section.id }}">
            <div class="swiper-wrapper">

    {% for block in section.blocks %}
          {% case block.type %}
           {% when 'slider' %}
    
               <div class="single_slider {{ block.id }} swiper-slide">
              <div class="row flex wrap align-center">
     
               {% if block.settings.img != blank %}
                 <style>
                   .{{ block.id }} .row .col-2{
                     max-width:50%;
                     width: 100%;
                     box-sizing: border-box;
                   }
                 </style>
                  <div class="col col-1">
                <img class="img_tag"
                  src="{{ block.settings.img | image_url }}"
                  alt="{{ block.settings.img.alt }}"
                  width="100"
                  height="100"
                >
                </div>
              {% endif %}
                  <div class="col col-2">
                    <div class="slider_content">
                       <div class="content_div">

                         
                  {% if block.settings.text != blank %}
           
                 <style>
                   .{{ block.id }} .row .col-1{
                     max-width:50%;
                     width: 100%;
                     box-sizing: border-box;
                   }
                 </style>
                    
                     <div class="text">{{ block.settings.text }}</div>
                    <div class="flex">
                          <div class="content">
                            <h3>{{ block.settings.name }}</h3>
                            <p>{{ block.settings.des }}</p>
                        </div>
                   <div class="profile_img">
                   {% if block.settings.profile_img != blank %}
                     <img 
                  src="{{ block.settings.profile_img | image_url }}"
                  alt="{{ block.settings.profile_img.alt }}"
                  width="100"
                  height="100"
                >
    
              {% endif %}
                     </div>
                    
                     </div>

                  {% endif %}
                         
                      </div>
                     </div>
                     </div>
            

                     </div>
   </div>
           
             {% endcase %}
      {% endfor %} 

    </div>
    <div class="swiper-button-next"></div>
    <div class="swiper-button-prev"></div>
    <div class="swiper-pagination"></div>
  </div>
 
</div>







    
  </div>
</section>



  {% for block in section.blocks %}
        {% case block.type %}
                {% when 'style' %}

  <script>
    var swiper = new Swiper(".mySwiper{{ section.id }}", {
        {% for block in section.blocks %}
        {% case block.type %}
                {% when 'mobail_style' %}
                      slidesPerView: {{ block.settings.slider_per_view }},
            {% endcase %}
      {% endfor %} 

      
        breakpoints: {
      769: {
        slidesPerView:{{ block.settings.slider_per_view }},
        slidesPerGroup: 1,
      },
    },
 
      navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
      pagination: {
        el: ".swiper-pagination",
         clickable: true,
      },

      spaceBetween: 20,
      autoplay: {
        delay: 5000,
        disableOnInteraction: false,
      },
      loop: true,
    });
  </script>
             {% endcase %}
      {% endfor %} 






{% schema %}
  {
    "name": "Shopc Slider",
    "settings": [
      
      {
  "type": "color",
   "label": "Background Color",
   "id": "bgcolor"
 },
 {
   "type": "header",
   "content": "Laptop Padding"
 },

 {
       "type": "range",
          "id": "laptop_top",
          "label": "Top",
          "min":0,
          "max":150,
          "step": 2,
        "unit": "px",
        "default":112
      },
      {
       "type": "range",
          "id": "laptop_bottom",
          "label": "Bottom",
          "min":0,
          "max":150,
        "unit": "px",
          "default":112,
          "step": 2
      },

       {
        "type": "header",
        "content": "Mobail Padding"
      },
      {
       "type": "range",
          "id": "mobail_top",
          "label": "Top",
          "min":0,
          "max":80,
          "step": 1,
        "unit": "px",
        "default":64
      },
      {
       "type": "range",
          "id": "mobail_bottom",
          "label": "Bottom",
          "min":0,
          "max":80,
        "unit": "px",
          "default":64,
          "step": 1
      },
      {
        "type": "header",
        "content": "Border Style"
      },
      {
        "type": "text",
        "id": "border_width",
        "label": "Border Width ( 1px 0px 1px 0px)",
        "default":"0"
      },
      {
        "type": "color",
        "id": "border_color",
        "default": "#d9d9d9",
        "label": "Border Color"
      },
      {
        "type": "select",
         "id": "border_style",
            "label": "Border Style",
            "default": "solid",
            "options": [
              {
                "label": "Solid",
                "value": "solid"
              },
              {
                "value": "dotted",
                "label": "Dotted"
              },
              {
                "label": "Dashed",
                "value": "dashed"
              },
              {
                "label": "Double",
                "value": "double"
              }
            ]
          }
    ],
    "blocks": [ 
      {
  "type": "title",
  "name": "Title",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title",
      "default":"The ShopLandingAI Solution"
    },
    {
      "type": "color",
      "id": "textcolor",
      "label": "Color",
      "default":"#000000"
    },
        {
       "type": "range",
          "id": "title_position",
          "label": "Position Title",
          "min":0,
          "max":5,
          "default":0,
          "step": 1
    },
     {
   "type": "header",
   "content": "Laptop Typography"
    },
     {
        "type": "select",
         "id": "text_align",
            "label": "Text Align",
            "default": "left",
            "options": [
              {
                "label": "left",
                "value": "left"
              },
              {
                "value": "center",
                "label": "center"
              },
              {
                "label": "right",
                "value": "right"
              }
            ]
          },
        {
       "type": "range",
          "id": "width",
          "label": "Width",
          "min":0,
          "max":100,
          "unit": "%",
          "default":100,
          "step": 1
      },     {
        "type": "select",
         "id": "align_content",
            "label": "Align Content",
            "default": "0px auto 0px 0px",
            "options": [
              {
                "label": "left",
                "value": "0px auto 0px 0px"
              },
              {
                "value": "0px auto",
                "label": "center"
              },
              {
                "label": "right",
                "value": "0px 0px 0px auto"
              }
            ]
          },
    {
       "type": "range",
          "id": "font_size",
          "label": "Font Size",
          "min":0,
          "max":100,
        "unit": "px",
          "default":50,
          "step": 1
      },
        {
       "type": "range",
          "id": "line_height",
          "label": "Line Height",
          "min":0,
          "max":100,
        "unit": "px",
          "default":60,
          "step": 1
      },

    {
        "type": "select",
         "id": "font_weight",
            "label": "Font Weight",
            "default": "500",
            "options": [
              {
                "label": "100",
                "value": "100"
              },
              {
                "value": "200",
                "label": "200"
              },
                {
                "label": "300",
                "value": "300"
              },
              {
                "value": "400",
                "label": "400"
              },
                  {
                "label": "500",
                "value": "500"
              },
              {
                "value": "600",
                "label": "600"
              },
              {
                "label": "700",
                "value": "700"
              },
              {
                "label": "800",
                "value": "800"
              },
               {
                "label": "900",
                "value": "900"
              },
               {
                "label": "Bold",
                "value": "bold"
              }
            ]
          },
          {
        "type": "range",
          "id": "margin_bottom",
          "label": "Margin Bottom",
          "min":0,
          "max":100,
           "unit": "px",
          "default":8,
          "step": 1
    },
         {
   "type": "header",
   "content": "Mobail Typography"
    },
    {
        "type": "select",
         "id": "mobail_text_align",
            "label": "Text Align",
            "default": "left",
            "options": [
              {
                "label": "left",
                "value": "left"
              },
              {
                "value": "center",
                "label": "center"
              },
              {
                "label": "right",
                "value": "right"
              }
            ]
          },
        {
       "type": "range",
          "id": "mobail_width",
          "label": "Width",
          "min":0,
          "max":100,
          "unit": "%",
          "default":100,
          "step": 1
      },     {
        "type": "select",
         "id": "mobail_align_content",
            "label": "Align Content",
            "default": "0px auto 0px 0px",
